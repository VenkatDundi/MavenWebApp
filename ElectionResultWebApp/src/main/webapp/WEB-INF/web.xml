<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>GetGenderController</servlet-name>
  	<display-name>GetGenderController</display-name>
  	<description></description>
  	<servlet-class>com.venkat.web.GetGenderController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>getCandidateController</servlet-name>
  	<display-name>getCandidateController</display-name>
  	<description></description>
  	<servlet-class>com.venkat.web.getCandidateController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GetGenderController</servlet-name>
  	<url-pattern>/getGenderResult</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>getCandidateController</servlet-name>
  	<url-pattern>/getCandidate</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
       <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
